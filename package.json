{
  "name": "@meveth/docs",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production npx next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "fmt": "npx prettier --config .prettierrc.js --write '**/*.{js,jsx,ts,tsx,mdx}'",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "baseline": "npx tsc --extendedDiagnostics",
    "explain": "npx tsc --explainFiles > explanation.txt",
    "trace": "npx tsc --traceResolution > resolution.txt",
    "profile": "node --trace-ic ./node_modules/typescript/lib/tsc.js --generateCpuProfile profile.cpuprofile -p tsconfig.json",
    "tracer": "mkdir -p tmp_folder/ && npx tsc -p ./tsconfig.json --generateTrace tmp_folder"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.7.3",
    "@headlessui/react": "^1.7.15",
    "@headlessui/tailwindcss": "^0.2.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "13.4.16",
    "@sindresorhus/slugify": "^2.1.1",
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "20.4.7",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/react-highlight-words": "^0.16.4",
    "@vercel/analytics": "^1.0.2",
    "acorn": "^8.8.1",
    "autoprefixer": "^10.4.7",
    "clsx": "^1.2.0",
    "fast-glob": "^3.3.0",
    "flexsearch": "^0.7.31",
    "framer-motion": "7.8.1",
    "mdast-util-to-string": "^4.0.0",
    "mdx-annotations": "^0.1.1",
    "next": "^13.4.19",
    "next-sitemap": "4.1.3",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.20.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.3.0",
    "shiki": "^0.14.4",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "unist-util-filter": "^4.0.1",
    "unist-util-visit": "^4.1.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@typescript/analyze-trace": "^0.10.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.16",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.2",
    "process-tracing": "^1.0.2",
    "sharp": "^0.32.0",
    "ts-jest": "^29.1.1",
    "tsutils": "^3.21.0",
    "typecheck.macro": "^0.7.3",
    "typescript-eslint-language-service": "^5.0.5"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all",
      "not IE > 0",
      "not samsung 4",
      "not and_uc 12.12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 2 Edge major versions ",
      "last 2 iOS major versions ",
      "not iOS 12.5"
    ]
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "minimumChangeThreshold": 0,
    "showDetails": true
  }
